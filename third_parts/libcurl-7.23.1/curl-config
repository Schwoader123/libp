prefix=/usr/local
exec_prefix=${prefix}
includedir=${prefix}/include

usage()
{
    cat <<EOF
Usage: curl-config [OPTION]

Available values for OPTION include:

  --built-shared says 'yes' if libcurl was built shared
  --ca        ca bundle install path
  --cc        compiler
  --cflags    pre-processor and compiler flags
  --checkfor [version] check for (lib)curl of the specified version
  --configure the arguments given to configure when building curl
  --features  newline separated list of enabled features
  --help      display this help and exit
  --libs      library linking information
  --prefix    curl install prefix
  --protocols newline separated list of enabled protocols
  --static-libs static libcurl library linking information
  --version   output version information
  --vernum    output the version information as a number (hexadecimal)
EOF

    exit $1
}

if test $# -eq 0; then
    usage 1
fi

while test $# -gt 0; do
    case "$1" in
    # this deals with options in the style
    # --option=value and extracts the value part
    # [not currently used]
    -*=*) value=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
    *) value= ;;
    esac

    case "$1" in
    --built-shared)
        echo yes
        ;;

    --ca)
	echo ""/etc/ssl/certs/ca-certificates.crt""
	;;

    --cc)
	echo "arm-eabi-gcc"
	;;

    --prefix)
	echo "$prefix"
	;;

    --feature|--features)
        for feature in SSL NTLM NTLM_WB ""; do
            test -n "$feature" && echo "$feature"
        done
	;;

    --protocols)
        for protocol in DICT FILE FTP FTPS GOPHER HTTP HTTPS IMAP IMAPS POP3 POP3S RTSP SMTP SMTPS TELNET TFTP; do
            echo "$protocol"
        done
	;;

    --version)
	echo libcurl 7.23.1
	exit 0
	;;

    --checkfor)
        checkfor=$2
        cmajor=`echo $checkfor | cut -d. -f1`
        cminor=`echo $checkfor | cut -d. -f2`
        # when extracting the patch part we strip off everything after a
	# dash as that's used for things like version 1.2.3-CVS
	cpatch=`echo $checkfor | cut -d. -f3 | cut -d- -f1`
        checknum=`echo "$cmajor*256*256 + $cminor*256 + ${cpatch:-0}" | bc`
        numuppercase=`echo 071701 | tr 'a-f' 'A-F'`
        nownum=`echo "obase=10; ibase=16; $numuppercase" | bc`

	if test "$nownum" -ge "$checknum"; then
	  # silent success
	  exit 0
	else
	  echo "requested version $checkfor is newer than existing 7.23.1"
	  exit 1
	fi
	;;

    --vernum)
	echo 071701
	exit 0
	;;

    --help)
	usage 0
	;;

    --cflags)
       	if test "X${prefix}/include" = "X/usr/include"; then
          echo ""
        else
          echo "-I${prefix}/include"
        fi
       	;;

    --libs)
	if test "X${exec_prefix}/lib" != "X/usr/lib" -a "X${exec_prefix}/lib" != "X/usr/lib64"; then
	   CURLLIBDIR="-L${exec_prefix}/lib "
	else
	   CURLLIBDIR=""
	fi
	if test "Xno" = "Xyes"; then
	  echo ${CURLLIBDIR}-lcurl -L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -lssl -lcrypto -L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -L/home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib -Wl,-rpath-link=out/target/product/m1ref/obj/lib -lc -lm -ldl /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtbegin_dynamic.o -Wl,--no-undefined /home/zhuxiaoqiang/android_hs/rootfs/prebuilt/linux-x86/toolchain/arm-eabi-4.2.1/lib/gcc/arm-eabi/4.2.1/interwork/libgcc.a /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtend_android.o -L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -L/home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib -Wl,-rpath-link=out/target/product/m1ref/obj/lib -lc -lm -ldl /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtbegin_dynamic.o -Wl,--no-undefined /home/zhuxiaoqiang/android_hs/rootfs/prebuilt/linux-x86/toolchain/arm-eabi-4.2.1/lib/gcc/arm-eabi/4.2.1/interwork/libgcc.a /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtend_android.o
	else
	  echo ${CURLLIBDIR}-lcurl -L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -L/home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib -Wl,-rpath-link=out/target/product/m1ref/obj/lib -lc -lm -ldl /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtbegin_dynamic.o -Wl,--no-undefined /home/zhuxiaoqiang/android_hs/rootfs/prebuilt/linux-x86/toolchain/arm-eabi-4.2.1/lib/gcc/arm-eabi/4.2.1/interwork/libgcc.a /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtend_android.o
	fi
	;;

    --static-libs)
	echo ${exec_prefix}/lib/libcurl.a -L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -lssl -lcrypto -L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -L/home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib -Wl,-rpath-link=out/target/product/m1ref/obj/lib -lc -lm -ldl /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtbegin_dynamic.o -Wl,--no-undefined /home/zhuxiaoqiang/android_hs/rootfs/prebuilt/linux-x86/toolchain/arm-eabi-4.2.1/lib/gcc/arm-eabi/4.2.1/interwork/libgcc.a /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtend_android.o -L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -L/home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib -Wl,-rpath-link=out/target/product/m1ref/obj/lib -lc -lm -ldl /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtbegin_dynamic.o -Wl,--no-undefined /home/zhuxiaoqiang/android_hs/rootfs/prebuilt/linux-x86/toolchain/arm-eabi-4.2.1/lib/gcc/arm-eabi/4.2.1/interwork/libgcc.a /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtend_android.o
	;;

    --configure)
      echo " '--host=arm-linux' 'CC=arm-eabi-gcc' 'CPPFLAGS=-I /home/zhuxiaoqiang/android_hs/rootfs/system/core/include -I /home/zhuxiaoqiang/android_hs/rootfs/hardware/libhardware/include   -I /home/zhuxiaoqiang/android_hs/rootfs/hardware/libhardware_legacy/include   -I /home/zhuxiaoqiang/android_hs/rootfs/hardware/ril/include   -I /home/zhuxiaoqiang/android_hs/rootfs/dalvik/libnativehelper/include   -I /home/zhuxiaoqiang/android_hs/rootfs/frameworks/base/include   -I /home/zhuxiaoqiang/android_hs/rootfs/frameworks/base/opengl/include   -I /home/zhuxiaoqiang/android_hs/rootfs/external/skia/include   -I /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/include   -I /home/zhuxiaoqiang/android_hs/rootfs/bionic/libc/arch-arm/include   -I /home/zhuxiaoqiang/android_hs/rootfs/bionic/libc/include   -I /home/zhuxiaoqiang/android_hs/rootfs/bionic/libstdc++/include   -I /home/zhuxiaoqiang/android_hs/rootfs/bionic/libc/kernel/common   -I /home/zhuxiaoqiang/android_hs/rootfs/bionic/libc/kernel/arch-arm   -I /home/zhuxiaoqiang/android_hs/rootfs/bionic/libm/include   -I /home/zhuxiaoqiang/android_hs/rootfs/bionic/libm/include/arch/arm   -I /home/zhuxiaoqiang/android_hs/rootfs/bionic/libthread_db/include   -I /home/zhuxiaoqiang/android_hs/rootfs/dalvik/libnativehelper/include/nativehelper -I /home/zhuxiaoqiang/android_hs/rootfs/external/curl -I /home/zhuxiaoqiang/android_hs/rootfs/external/curl/include -I /home/zhuxiaoqiang/android_hs/rootfs/external/openssl -I /home/zhuxiaoqiang/android_hs/rootfs/external/openssl/include' 'CFLAGS=-nostdlib -fno-exceptions -Wno-multichar -Wno-long-long -msoft-float -fpic -ffunction-sections -funwind-tables -fstack-protector -fno-short-enums -march=armv5te -mtune=xscale -D__ARM_ARCH_5__ -D__ARM_ARCH_5T__ -D__ARM_ARCH_5E__ -D__ARM_ARCH_5TE__ -include /home/zhuxiaoqiang/android_hs/rootfs/system/core/include/arch/linux-arm/AndroidConfig.h -I /home/zhuxiaoqiang/android_hs/rootfs/system/core/include/arch/linux-arm/ -mthumb-interwork -DANDROID -fmessage-length=0 -W -Wall -Wno-unused -Winit-self -Wpointer-arith -Werror=return-type -Werror=non-virtual-dtor -Werror=address -Werror=sequence-point -DSK_RELEASE -DNDEBUG -g -Wstrict-aliasing=2 -finline-functions -fno-inline-functions-called-once -fgcse-after-reload -frerun-cse-after-loop -frename-registers -DNDEBUG -UDEBUG -DANDROID -fmessage-length=0 -W -Wall -Wno-unused -Winit-self -Wpointer-arith  -Werror=return-type -Werror=non-virtual-dtor -Werror=address -Werror=sequence-point -DNDEBUG -UDEBUG -mthumb -Os -fomit-frame-pointer -fno-strict-aliasing -finline-limit=64  -Wno-endif-labels -Wno-import -Wno-format -fno-strict-aliasing -fvisibility=hidden -Darm   -MD' 'SDFLAGS=-s -Wl,-soname,libcurl.so -Wl,-shared,-Bsymbolic -Wl,-T,/home/zhuxiaoqiang/android_hs/rootfs/build/core/armelf.xsc -Wl,--gc-setions -lc -lm' 'LIBS=-L/home/zhuxiaoqiang/android_hs/rootfs/external/openssl/lib -L/home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib -Wl,-rpath-link=out/target/product/m1ref/obj/lib -lc -lm -ldl /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtbegin_dynamic.o -Wl,--no-undefined /home/zhuxiaoqiang/android_hs/rootfs/prebuilt/linux-x86/toolchain/arm-eabi-4.2.1/lib/gcc/arm-eabi/4.2.1/interwork/libgcc.a /home/zhuxiaoqiang/android_hs/rootfs/out/target/product/m1ref/obj/lib/crtend_android.o' '--with-ssl=/home/zhuxiaoqiang/android_hs/rootfs/external/openssl' 'host_alias=arm-linux'"
    ;;

    *)
        echo "unknown option: $1"
	usage 1
	;;
    esac
    shift
done

exit 0
